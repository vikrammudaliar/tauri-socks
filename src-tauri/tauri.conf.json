{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "pp-socks",
    "version": "0.1.0",
    "identifier": "com.pp-socks.app",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "pp-socks",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "resources": ["keys/id_rsa.pem"],
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
